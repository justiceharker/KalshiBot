# Kalshi API Credentials
# Copy this file to .env and fill in your actual values
# NEVER commit .env to git — it's in .gitignore

KALSHI_KEY_ID=your-api-key-id-here
KALSHI_PRIVATE_KEY_PATH=kalshi_key.pem

# Trading Strategy Parameters (optional — use defaults if not set)
MR_WINDOW=15
MR_THRESHOLD=5.0
MR_MAX_HOLD=3600
MR_REFRESH=2

# Logging
KALSHI_LOG_FILE=trading_log.csv

# Test Mode (set to true to run without executing real orders)
PAPER_TRADING=false
